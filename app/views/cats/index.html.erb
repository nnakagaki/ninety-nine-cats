<html>
  <header>
    <title>99Cats</title>
  </header>

  <h1>All the Cats!</h1>
  <body>
    <ul>
      <% @cats.each do |cat| %>
        <li><a href=<%= cat_url(cat) %> ><%= cat.name %></a></li>
      <% end %>
    </ul>

    <br>

    <a href="<%= new_cat_url %>">Create a new cat!</a>

    <br>

    <a href="<%= new_cat_rental_request_url %>">Rent a cat!</a>
  </body>
</html>


